{
  "name": "teksavvy-dashboard",
  "description": "Dashboard for tracking TekSavvy bandwidth usage",
  "author": "Maxime Gibeau <maxime@gibeau.email>",
  "version": "0.1.0",
  "private": true,
  "main": "electron.js",
  "dependencies": {
    "express": "^4.16.2",
    "http-proxy-middleware": "^0.17.4"
  },
  "proxy": {
    "/api": {
      "target": "https://api.teksavvy.com/web/Usage",
      "pathRewrite": {
        "^/api": ""
      },
      "secure": false,
      "headers": {
        "TekSavvy-APIKey": "UNDEFINED_API_KEY"
      }
    }
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "devDependencies": {
    "ant-design-pro": "^0.3.1",
    "antd": "^3.1.0",
    "babel-plugin-import": "^1.6.3",
    "electron": "^1.7.10",
    "electron-builder": "^19.52.1",
    "local-storage": "^1.4.2",
    "moment": "^2.20.1",
    "numeral": "^2.0.6",
    "react": "^16.2.0",
    "react-app-rewire-less": "^2.1.0",
    "react-app-rewired": "^1.4.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "string-hash": "^1.1.3"
  },
  "build": {
    "appId": "com.maxjbo.teksavvy-dashboard",
    "win": {
      "icon": "public/favicon.ico"
    },
    "directories": {
      "buildResources": "public"
    },
    "extends": null
  }
}
